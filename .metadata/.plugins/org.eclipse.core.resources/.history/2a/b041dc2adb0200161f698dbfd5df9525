package j_7;

public class j_7 {
	public static void main(String args[]) {
		for (int i = 1; i <=5; i++) {
			for (int j = 1; j <= i; j++) {
				System.out.print(j);
			}
			System.out.println("\n");
		}
		for (int k = 5; k < 1; k--) {
			for (int l = 5; l >= k; k--) {
				System.out.print(l);
			}
			System.out.println("\n");
		}
	}

}
